page{
    background-color: #eee;
}
.supportBankView{
    width: calc(100% - 50px);
    height: 535px;
    margin: 20px auto;
    border-radius: 5px;
    background-color: #fff;
}
.supportBank{
    width: 100%;
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
}
.supportBankList{
    width: 49%;
    border-bottom: 1px solid #eee;
    height: 55px;
    position: relative;
    z-index: 1;
}
.supportBankList:nth-of-type(odd){
    border-right: 1px solid #eee;
    box-sizing: border-box;
}
.backListImg{
    width: 60%;
    height: 20px;
    position: absolute;
    top: 18px;
    left: 20%;
    z-index: 5;
}
.noSup{
    font-size: 12px;
    color: red;
    position: absolute;
    bottom: 8px;
    right: 22px;
    z-index: 10;
}
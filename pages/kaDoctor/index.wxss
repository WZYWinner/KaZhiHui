page{
    background-color: #eee;
}
.kaDoctor{
    width: calc(100% - 30px);
    margin: 10px auto;
}
.basicInfo{
    width: 100%;
}
.infoTitle{
    color: #CBB585;
    font-size: 14px;
    margin-bottom: 10px;
    position: relative;
}
.record{
    width: 60px;
    text-align: center;
    height: 20px;
    line-height: 20px;
    position: absolute;
    top: 0;
    right: 4px;
    color: #fff;
    background-color: #CBB585;
    border-radius: 10px;
    font-size: 12px;
}
.field-index--van-field{
    margin-top: 5px;
    border-radius: 30px;
    border: 1px solid #eee;
}
.van-button{
    border-radius: 15px !important;
}
.van-button--primary{
    background-color: #CBB585 !important;
    border: none !important;
}
.shouquanshu{
    width: 100%;
    text-align: center;
    font-size: 11px;
    margin-top: 20px;
}
.van-checkbox{
    width: 191px !important;
    margin: 0 auto !important;
    
}
.van-checkbox__icon--round{
    width: 10px !important;
    height: 10px !important;
    margin-top: 5px !important;
    transition: .1s;
    background-color: #fff
}
.van-checkbox__label{
    margin-left: 5px !important;
}
.shouquanText{
    color: #CBB585;
}
.searchPri{
    width: 100%;
    margin-top: 4px;
    text-align: center;
}
.searchView{
    width: 100%;
}
.searchBut{
    height: 30px;
    text-align: center;
    line-height: 30px;
    font-size: 14px;
    background-color: #CBB585;
    border: none;
    border-radius: 15px;
    margin-top: 20px;
}
.searchKnow{
    width: calc(100% - 30px);
    padding: 10px 15px;
    border-radius: 8px;
    background-color: #fff;
    margin-top: 25px;
}
.knowTitle{
    font-size: 13px;
    margin-bottom: 5px;
}
.knowInfo{
    font-size: 11px;
}
.van-cell__title{
    font-size: 12px !important;
}
.van-field__input{
    font-size: 12px !important;
}
picker{
    width: 100% !important;
    /* background-color: #555 !important; */
    display: flex !important;
    flex-wrap: nowrap !important;
    justify-content: space-between !important;
    position: relative !important;
}
/* 上传借记卡 */
.jiejikaImg {
    width: calc(100% - 40px);
    margin: 20px auto;
    height: 130px;
    position: relative;
    background-color: #E8E8E8;
  }
  
  .jiejikaImg .q-image {
    width: 100%;
    height: 100%;
  }
  
  .q-imageNo {
    width: 30px;
    height: 30px;
    display: block;
    margin: 0px auto;
    padding-top: 36px;
  }
  
  .upLoadBtn {
    position: absolute;
    width: 100%;
    background-color: rgba(255, 255, 255, 0);
    height: 100%;
    text-align: center;
    z-index: 90;
    border: none;
    outline: none;
  }

  .imgText {
    position: relative;
    z-index: 9;
    font-size: 13px;
    text-align: center;
    margin-top: 5px;
}
.guifan{
    width: calc(100% - 40px);
    margin: 15px auto 50px; 
}
.guifanTitle{
    font-size: 13px;
    margin-bottom: 5px;
}
.guifandetail{
    font-size: 11px;
    margin-top: 3px;
}
.tijiaoBut{
    width: 100%;
    height: 30px;
    line-height: 30px;
    text-align: center;
    background-color: #CBB585;
    font-size: 14px;
    position: fixed;
    bottom: 0;
    left: 0;
}
.jiaoyiXinyong{
    width: 100%;
    padding: 15px 15px 0;
    background-color: #fff;
}

.xinyongview{
    width: 100%;
    border-bottom: 1px solid #eee;
    box-sizing: border-box;
    height: 30px;
    display: flex;
    flex-wrap: nowrap;
    justify-content: space-between;
    font-size: 12px;
    color: #555;
}
.jiaoyiXinyongTitle{
    width: 20%;
    position: relative;
}
.jiaoyiXinyongKahao{
    position: relative;
    z-index: 5;
    margin-right: 30px;
}
.Kahaoinfo{
    margin-right: 15px;
    color: #999;
}
.rightMore{
    position: absolute;
    top: 3px;
    right: 0px;
    width: 12px;
    height: 12px;
    z-index: 6;
}
.biaoji{
    display: block;
    position: absolute;
    color: red;
    top: -1px;
    left: -7px;
}

.jiaoyiXinyongTitlePicker{
    width: 235px;
    position: relative;
}
.jiaoyiXinyongKahaoPicker{
    position: absolute;
    z-index: 5;
    margin-right: 30px;
    right: 0;
    top: -2px;
}
.tishitext{
    width: calc(100% - 30px);
    margin-left: 10px;
    margin-top: 10px;
    font-size: 12px;
    color: #999;
}
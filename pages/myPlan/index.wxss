    padding-bottom: 10px;

}
.danhao{
    width: 75%;
    font-size: 12px;
    color: #CBB585;
    margin-bottom: 10px;
}
.price{
    width: 75%;
    font-size: 18px;
    font-weight: bold;
}
.num{
    width: 35%;
    font-size: 11px;
    color: #555;
    margin-top: 5px;
    position: relative;
}
.cishu{
    position: absolute;
    right: 0;
    top: 0;
}
.benjin{
    width: 75%;
    font-size: 11px;
